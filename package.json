{
  "name": "mbgl-tile-renderer",
  "version": "1.0.0",
  "description": "A tile renderer for Maplibre GL JS",
  "type": "module",
  "main": "./src/cli.js",
  "scripts": {
    "lint": "prettier --write ./src",
    "prepare": "husky install",
    "test": "jest --coverage --silent"
  },
  "bin": {
    "mbgl-tile-renderer": "./src/cli.js"
  },
  "dependencies": {
    "@azure/storage-queue": "^12.16.0",
    "@mapbox/mbtiles": "^0.12.1",
    "@maplibre/maplibre-gl-native": "^5.3.1",
    "axios": "^1.6.5",
    "commander": "^11.1.0",
    "fs": "^0.0.1-security",
    "p-limit": "^5.0.0",
    "path": "^0.12.7",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.1",
    "dotenv-flow": "^4.1.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "pixelmatch": "^5.3.0",
    "prettier": "^3.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "prettier --write ./src",
      "git add"
    ]
  },
  "repository": "https://github.com/conservationmetrics/mbgl-tile-renderer",
  "license": "MIT"
}
